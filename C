#include <stdio.h>
int main(void)
{
   //读入10个成绩，判断几个不及格
	int score[10];
	int c1=0,c2=0;
	int i;

	printf("请输入10个学生成绩：");
	scanf("%d", &score[i]);
	for(i = 0; i < 10; i++)
	{
		if(score[i] >= 60)
			c1++;
		else
			c2++;
	}
	printf("及格人数有%d个\n", c1); 
	return 0;
}


